{
    "extends": [
        "web-recommended"
    ],
    "rules": {
        "no-unused-vars": "error",
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": ["error", 2]
    },
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es2021": true
    }
}